<?xml version="1.0" encoding="UTF-8"?>
<cronentries>
	<cron>
		<url>/monitoringgae/mail</url>
		<description>Mail job</description>
		<schedule>every 30 minutes</schedule>
	</cron>

	<cron>
		<url>/monitoringgae/gather</url>
		<description>Gather job</description>
		<schedule>every 1 minutes</schedule>
	</cron>

	<cron>
		<url>/monitoringgae/job</url>
		<description>Repopulate the cache every 30 minutes</description>
		<schedule>every 30 minutes</schedule>
	</cron>
	
	<cron>
		<url>/monitoringgae/token</url>
		<description>Repopulate the token every 6 hours</description>
		<schedule>every 6 hours</schedule>
	</cron>
	
</cronentries>
